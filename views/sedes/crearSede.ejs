                                                <% 
                                       
                                                    for (let i = 0; i < paises.length; i++) {
                                                    const paises2 = paises[i]; 
                                                        
                                                %>
                                                    <option value="<%= paises2.code %>"><%= paises2.name %></option>
                                                <% } %>
                                                
                                              </select>
                                        </div>
                                            
