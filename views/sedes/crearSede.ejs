                                                            departamentos.sort(function (a, b) {
                                                                if (a.Nombre > b.Nombre) {
                                                                    return 1
                                                                } else if (a.Nombre < b.Nombre) {
                                                                    return -1
                                                                } else {
                                                                    return 0
                                                                }
                                                            })
